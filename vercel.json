{
	"cleanUrls": true,
	"trailingSlash": true,
	"rewrites": [
		{
			"source": "/labs/:splat*",
			"destination": "https://jelias-labs.vercel.app/labs/:splat*"
		},
		{
			"source": "/:splat*",
			"destination": "https://jelias-labs.vercel.app/:splat*",
			"has": [
				{
					"type": "host",
					"value": "labs.jelias.me"
				}
			]
		}
	]
}
