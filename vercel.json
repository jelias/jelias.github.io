{
	"cleanUrls": true,
	"redirects": [
		{
			"source": "/labs/:splat*",
			"destination": "https://jelias-labs.vercel.app/labs/:splat*",
			"permanent": true
		},
		{
			"source": "/labs",
			"destination": "https://jelias-labs.vercel.app/labs/",
			"permanent": true
		},
		{
			"source": "/:splat*",
			"destination": "https://jelias.me/labs/:splat*",
			"has": [
				{
					"type": "host",
					"value": "labs.jelias.me"
				}
			],
			"permanent": true
		}
	],
	"rewrites": [
		{
			"source": "/labs/:splat*",
			"destination": "https://jelias-labs.vercel.app/labs/:splat*"
		},
		{
			"source": "/labs",
			"destination": "https://jelias-labs.vercel.app/labs/"
		}
	]
}
